{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/npm"
      },
      {
        "src": "src/**/*.ts*",
        "use": "@vercel/build-utils"
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  